{
  "name": "react-redux-hot-boilerplate",
  "version": "1.0.0",
  "description": "React Redux Hot Boilerplate website",
  "scripts": {
    "dev": "babel-node src/server/index.js",
    "lint": "eslint src",
    "prebuild": "rm -rf dist",
    "build": "npm run build:client && npm run build:server",
    "build:server": "babel src/server -d dist/server",
    "build:client": "NODE_ENV=production webpack -p --progress --config src/server/webpack.config.production.js",
    "start": "NODE_ENV=production node dist/server/index.js",
    "test": "npm run lint && npm run mocha",
    "mocha": "NODE_PATH=./src nyc mocha"
  },
  "dependencies": {
    "express": "^4.16.3",
    "serve-favicon": "^2.4.5",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "expect": "^22.4.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.7",
    "mini-css-extract-plugin": "^0.2.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "style-loader": "^0.20.3"
  }
}
